<!--SignUp Form goes here -->
<div id="signup-form">
  <div class="container text-center" id="sign-up">
    <div class="sign-in row">
      <div class="col-md-6 col-md-offset-3">
        <h1 id="login-text">Sign Up</h1>
        <hr>
        <form id="signup-form">
          <div class="form-group">
            <p class="field"><b>Email: </b></p>
            <input type="email" name="email" class="form-control" placeholder="Email" autofocus require>
          </div>
          <div class="form-group">
            <p class="field"><b>Password: </b></p>
            <input type="password" name="password" class="form-control" placeholder="Password" autofocus require>
          </div>
          <div class="form-group">
            <input type="submit" value="SignUp" class="btn btn-primary login-btn">
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

</body>
